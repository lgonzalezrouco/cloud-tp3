# Example Terraform variables file
# Copy this file to terraform.tfvars and fill in your values
# NEVER commit terraform.tfvars to git!

# AWS Configuration
aws_region = "us-east-1"

# Application Configuration
app_name = "MatchMarket"

# Database Configuration
db_name     = "matchmarket"
db_username = "postgres"
db_password = "CHANGE-ME-TO-A-STRONG-PASSWORD"  # IMPORTANT: Use a strong password!

# S3 Configuration
s3_bucket_name = "your-unique-bucket-name-here"  # Must be globally unique

# Docker Configuration
use_dockerhub   = false
dockerhub_image = "emin364/cloud-backend:latest"

